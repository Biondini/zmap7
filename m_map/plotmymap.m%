
 [existFlag,figNumber]=figflag('Lambert Map',1);

       if existFlag == 0;  ac3 = 'new'; overmap;   end
       if existFlag == 1,
          figure(to1)
          delete(gca); delete(gca);delete(gca)
       end

 l  = get(h1,'XLim');
   s1 = l(2); s2 = l(1);
   l  = get(h1,'YLim');
   s3 = l(2); s4 = l(1);

m_proj('lambert','long',[s2 s1],'lat',[s4 s3]);
